package com.mycom.basic.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.RequestMapping;

@Controller
public class HomeController {

	
	@RequestMapping("/")
	public String home() {
		return "home.html";
	}
	
	@RequestMapping("/user/createAccount")
	public String createAccount() {
		return "createAccount.html";
	}
	
	@RequestMapping("/user/userDashboard")
	public String userDashboard() {
		return "userDashboard.html";
	}
	
	@RequestMapping("/user/modifyAccount")
	public String user() {
		return "modifyAccount.html";
	}
	
	@RequestMapping("/board/articleDetail")
	public String board() {
		return "articleDetail.html";
	}
	
	@RequestMapping("/board/articleList")
	public String articleList() {
		return "articleList.html";
	}
	
	@RequestMapping("/board/articleDetail")
	public String board() {
		return "articleDetail.html";
	}
}
